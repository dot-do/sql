{
  "name": "@dotdo/lake.do",
  "version": "0.1.0",
  "description": "Client SDK for DoLake - Lakehouse on Cloudflare Workers",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "lakehouse",
    "analytics",
    "cloudflare",
    "workers",
    "client",
    "sdk",
    "capnweb",
    "rpc",
    "cdc"
  ],
  "author": "dot.do",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dot-do/sql.git",
    "directory": "packages/lake.do"
  },
  "dependencies": {
    "@dotdo/sql.do": "workspace:*"
  },
  "devDependencies": {
    "typescript": "^5.7.0",
    "vitest": "^3.0.0"
  }
}
