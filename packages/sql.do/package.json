{
  "name": "sql.do",
  "version": "0.1.0",
  "description": "Client SDK for DoSQL - SQL database on Cloudflare Workers",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js"
    },
    "./experimental": {
      "types": "./dist/experimental.d.ts",
      "import": "./dist/experimental.js"
    },
    "./auth": {
      "types": "./dist/auth.d.ts",
      "import": "./dist/auth.js"
    },
    "./cli": {
      "types": "./dist/cli-helpers.d.ts",
      "import": "./dist/cli-helpers.js"
    },
    "./rpc": {
      "types": "./dist/rpc-transport.d.ts",
      "import": "./dist/rpc-transport.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "sql",
    "database",
    "cloudflare",
    "workers",
    "client",
    "sdk",
    "capnweb",
    "rpc"
  ],
  "author": "dot.do",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dot-do/sql.git",
    "directory": "packages/sql.do"
  },
  "dependencies": {
    "@dotdo/sql-types": "workspace:*"
  },
  "peerDependencies": {
    "rpc.do": "^0.2.0",
    "oauth.do": "^0.2.0",
    "@dotdo/cli": "^0.2.0"
  },
  "peerDependenciesMeta": {
    "rpc.do": { "optional": true },
    "oauth.do": { "optional": true },
    "@dotdo/cli": { "optional": true }
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.0.0",
    "typescript": "^5.7.0",
    "vitest": "^3.0.0"
  }
}
